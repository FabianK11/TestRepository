FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.3";
    object      cellDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    stationaryWalls
    {
        type            cellMotion;
        value           uniform (0 0 0);
    }
    atmosphere
    {
        type            cellMotion;
        value           uniform (0 0 0);
    }
    floatingObject
    {
        type            cellMotion;
        value           nonuniform 0();
    }
}


// ************************************************************************* //
